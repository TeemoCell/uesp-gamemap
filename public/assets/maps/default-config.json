{
    // General map info
    "mapTitle": "UESP Gamemap", // replace all of these when making new map config
    "wikiNamespace": "",
    "wikiURL": "//en.uesp.net/wiki/",
    "database" : "",
    "feedbackURL" : "https://github.com/uesp/uesp-gamemap/issues",
    "bgColour" : "#000000",
    "customCSS" : false,

    // Map options
    "defaultZoomLevel" : 2,
    "defaultXPos" : 0,
    "defaultYPos" : 0,

    // Map coordinate system info
    // 0: XY (Default)
    // 1: Normalised
    // 2: Worldspace
    "coordType" : 0,

    // Map tile info
    "tileSize" : 256,
    "tileLayers" : [
        "default"
    ],

    // Map world info
    "defaultWorldID" : 1,
    "minWorldID" : 0,
    "maxWorldID" : 20000,

    // Map icon info
    "iconSize" : 32,
    "icons": {
        // icon list goes here...
    },

    // Map grid info
    "cellSize" : 128, // size of a cell at full zoom on the map, in pixels
    "gridLineWidth" : 2,
    "gridLineColour" : "orange",
    "gridLabelColour" : "cyan",
    "gridStart" : [0, 0], //cell coords of the top leftmost point (default: 0 , 0);
    "gridShowLabels" : true,
    "gridShowLabelZoom" : 3,

    // core (do not copy or alter):
    "baseTileURL" : "//maps.uesp.net/",
    "assetsPath" : "assets/maps/"
}

